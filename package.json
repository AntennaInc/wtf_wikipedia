{
  "name": "wtf_wikipedia",
  "description": "parse wikiscript into json",
  "version": "7.8.1",
  "author": "Spencer Kelly <spencermountain@gmail.com> (http://spencermounta.in)",
  "repository": {
    "type": "git",
    "url": "git://github.com/spencermountain/wtf_wikipedia.git"
  },
  "main": "src/index.js",
  "unpkg": "builds/wtf_wikipedia.min.js",
  "module": "builds/wtf_wikipedia.mjs",
  "type": "commonjs",
  "sideEffects": false,
  "types": "types/index.d.ts",
  "scripts": {
    "build": "node ./scripts/version.js && rollup -c",
    "start": "node ./scripts/demo.js",
    "coverage": "node scripts/coverage.js",
    "test": "TESTENV=dev tape ./tests/*.test.js | tap-dancer",
    "test:fetch": "TESTENV=dev tape ./tests/fetch/*.test.js | tap-dancer",
    "watch": "amble ./scratch.js",
    "lint": "eslint ./src/",
    "test:types": "ts-node ./tests/types/index.ts | tap-dancer"
  },
  "bin": {
    "wtf_wikipedia": "./bin/wtf.js"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "builds",
    "api",
    "src",
    "types",
    "bin"
  ],
  "keywords": [
    "wikipedia",
    "wikimedia",
    "wikipedia markup",
    "wikiscript"
  ],
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "cross-fetch": "3.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/preset-env": "7.8.4",
    "@rollup/plugin-alias": "^3.0.1",
    "amble": "0.0.7",
    "codecov": "3.6.5",
    "nyc": "15.0.0",
    "rollup": "1.31.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-filesize-check": "0.0.1",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.2.0",
    "tap-dancer": "0.2.0",
    "tape": "4.13.0"
  },
  "eslintIgnore": [
    "builds/*.js"
  ],
  "license": "MIT"
}
